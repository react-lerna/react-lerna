{
  "name": "react-lerna",
  "version": "0.1.0",
  "scripts": {
    "dev:webapp": "cd apps/webapp && npm run dev",
    "storybook": "cd packages/ui-components && npm start",
    "lerna:bootstrap": "cd apps/webapp && lerna bootstrap",
    "lerna:changed": "lerna changed",
    "lerna:publish": "lerna publish",
    "lerna:ls": "lerna ls",
    "nuke": "lerna clean && rm -rf node_modules && rm -rf packages/ui-components/dist",
    "test": "lerna run test",
    "clear": "rm -rf packages/ui-components/dist",
    "build": "lerna exec --scope @react-lerna/ui-components -- yarn build"
  },
  "dependencies": {},
  "devDependencies": {
    "lerna": "^3.16.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/react-lerna/react-lerna.git"
  },
  "author": "Jo√£o Belo <joaobelo.dev@gmail.com>",
  "license": "ISC",
  "homepage": "https://github.com/react-lerna/react-lerna#readme",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
